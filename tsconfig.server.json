{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "esnext",
    "target": "es2017",
    "outDir": "./dist",
    "moduleResolution": "Node",
    "strict": true,
    "noEmit": false,
    "isolatedModules": false
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "include": ["server.mts"]
}
